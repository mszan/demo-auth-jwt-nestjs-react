{
  "name": "kraftapp-backend",
  "version": "1.0.0",
  "description": "",
  "main": "index.ts",
  "type": "module",
  "scripts": {
    "build": "tsc --build tsconfig.json",
    "format": "prettier --write .",
    "start": "mikro-orm-esm migration:up && node dist/src/index.js",
    "test:watch": "kill-port 4000 && vitest --api.port 4000 --coverage --ui",
    "test:debug": "vitest --no-file-parallelism --inspect-brk=0.0.0.0:9001",
    "type-check": "tsc --noEmit --skipLibCheck",
    "watch": "tsx watch --inspect=0.0.0.0:9000 --clear-screen=false src/index.ts"
  },
  "dependencies": {
    "@mikro-orm/cli": "6.4.10",
    "@mikro-orm/core": "6.4.10",
    "@mikro-orm/migrations": "6.4.10",
    "@mikro-orm/postgresql": "6.4.10",
    "@mikro-orm/reflection": "6.4.10",
    "@mikro-orm/seeder": "6.4.10",
    "@nestjs/common": "11.0.11",
    "@nestjs/core": "11.0.11",
    "@nestjs/jwt": "11.0.0",
    "@nestjs/passport": "11.0.5",
    "@nestjs/platform-express": "11.0.11",
    "@nestjs/swagger": "11.0.6",
    "bcrypt": "5.1.1",
    "body-parser": "2.2.0",
    "class-transformer": "0.5.1",
    "class-validator": "0.14.1",
    "cookie-parser": "1.4.7",
    "helmet": "8.0.0",
    "joi": "17.13.3",
    "passport": "0.7.0",
    "passport-jwt": "4.0.1",
    "passport-local": "1.0.0",
    "reflect-metadata": "0.2.2",
    "rxjs": "7.8.2",
    "ts-node": "10.9.2",
    "uuid": "11.1.0"
  },
  "devDependencies": {
    "@mikro-orm/nestjs": "^6.1.1",
    "@nestjs/cli": "11.0.5",
    "@nestjs/testing": "11.0.12",
    "@types/bcrypt": "5.0.2",
    "@types/cookie-parser": "1.4.8",
    "@types/lodash": "4.17.16",
    "@types/node": "22.13.10",
    "@types/passport-jwt": "4.0.1",
    "@types/passport-local": "1.0.38",
    "@types/reflect-metadata": "0.1.0",
    "@types/supertest": "6.0.3",
    "@types/uuid": "10.0.0",
    "@vitest/coverage-v8": "^3.1.2",
    "@vitest/ui": "^3.0.9",
    "node-mocks-http": "1.17.2",
    "kill-port": "2.0.1",
    "supertest": "7.1.0",
    "tsx": "4.19.3",
    "typescript": "5.8.2",
    "vitest": "^3.0.9",
    "vitest-mock-extended": "^3.0.1"
  },
  "mikro-orm": {
    "useTsNode": true,
    "tsConfigPath": "./tsconfig.json",
    "configPaths": [
      "./src/modules/orm/orm.config.ts",
      "./dist/src/modules/orm/orm.config.js"
    ]
  }
}